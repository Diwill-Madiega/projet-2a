{% extends 'base.html.twig' %}

{% block title %}Gamme{% endblock %}

{% block body %}
    <h1>Gamme</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ gamme.id }}</td>
            </tr>
            <tr>
                <th>Name</th>
                <td>{{ gamme.name }}</td>
            </tr>
            <tr>
                <th>Description</th>
                <td>{{ gamme.description }}</td>
            </tr>
            <tr>
                <th>Owner</th>
                <td>{{ gamme.owner.email }}</td>
            </tr>
            <tr>
                <th>Piece</th>
                <td>{{ gamme.piece.name }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('app_gamme_index') }}">back to list</a>

    <a href="{{ path('app_gamme_edit', {'id': gamme.id}) }}">edit</a>

    {{ include('gamme/_delete_form.html.twig') }}
{% endblock %}
