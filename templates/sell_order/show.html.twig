{% extends 'base.html.twig' %}

{% block title %}SellOrder{% endblock %}

{% block body %}
    <h1>SellOrder</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ sell_order.id }}</td>
            </tr>
            <tr>
                <th>Name</th>
                <td>{{ sell_order.name }}</td>
            </tr>
            <tr>
                <th>Date</th>
                <td>{{ sell_order.date ? sell_order.date|date('Y-m-d H:i:s') : '' }}</td>
            </tr>
            <tr>
                <th>Price</th>
                <td>{{ sell_order.price }}</td>
            </tr>
            <tr>
                <th>Detail</th>
                <td>{{ sell_order.detail }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('app_sell_order_index') }}">back to list</a>

    <a href="{{ path('app_sell_order_edit', {'id': sell_order.id}) }}">edit</a>

    {{ include('sell_order/_delete_form.html.twig') }}
{% endblock %}
